source 'https://rubygems.org'

gem 'rails', '4.1.2'

# Pre-processors must be included here for Alongslide
gem 'haml'
gem 'sass'
gem 'coffee-script'

# JS dependencies must be included here and not by Alongslide, so that Alongslide
# doesn't overwrite or redundantly load the Rails app's own dependencies.
gem 'jquery-rails'
gem 'underscore-rails'

gem 'alongslide'
